<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewData["Title"] - BalimOOn Indonesia</title>

    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/ThemeForest/vendors/bower-components/animate.css/animate.min.css" />
    <link rel="stylesheet" href="~/ThemeForest/vendors/bower-components/material-design-iconic-font/dist/css/material-design-iconic-font.min.css" />
    <link rel="stylesheet" href="~/ThemeForest/vendors/bower-components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" />
    <link rel="stylesheet" href="~/ThemeForest/vendors/bower-components/google-material-color/dist/palette.css" />

    <!-- CSS -->
    <link rel="stylesheet" href="~/ThemeForest/css/app.min.1.css" />
    <link rel="stylesheet" href="~/ThemeForest/css/app.min.2.css" />

    <!-- Custom CSS -->
    @RenderSection("Styles", required: false)

</head>
<body data-ma-header="blue">
    <header id="header" class="media">
        <div class="pull-left h-logo">
            <a asp-controller="Home" asp-action="Index" class="hidden-xs">
                BalimOOn
                <small>BalimOOn Liqueurs</small>
            </a>
            <div class="menu-collapse" data-ma-action="sidebar-open" data-ma-target="main-menu">
                <div class="mc-wrap">
                    <div class="mcw-line top palette-White bg"></div>
                    <div class="mcw-line center palette-White bg"></div>
                    <div class="mcw-line bottom palette-White bg"></div>
                </div>
            </div>
        </div>
    </header>
    <section id="main">
        @RenderBody()
        <footer id="footer">
            Copyright &copy; BalimOOn Liqueurs & Material Admin 2015


        </footer>
    </section>
    <!-- Page Loader -->
    <div class="page-loader palette-Blue bg">
        <div class="preloader pl-xl pls-white">
            <svg class="pl-circular" viewBox="25 25 50 50">
                <circle class="plc-path" cx="50" cy="50" r="20" />
            </svg>
        </div>
    </div>

    <!--Library JavaScripts-->
    <script type="text/javascript" src="~/ThemeForest/vendors/bower-components/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/ThemeForest/vendors/bower-components/bootstrap/dist/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/ThemeForest/vendors/bower-components/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.concat.min.js"></script>
    <script type="text/javascript" src="~/ThemeForest/vendors/bower-components/Waves/dist/waves.min.js"></script>
    <script type="text/javascript" src="~/ThemeForest/vendors/bootstrap-growl/bootstrap-growl.min.js"></script>


    <!--Script Function-->
    <script type="text/javascript" src="~/ThemeForest/js/functions.js"></script>
    <script type="text/javascript" src="~/ThemeForest/js/actions.js"></script>

    <!-- Custom Scripts-->
    @RenderSection("Scripts", required: false)
</body>
</html>